							"listen": "prerequest",
							"script": {
								"exec": [
									" const main = async () => {",
									"    const api = new API(pm);",
									"    const rnd = new RandomUtils();",
									"",
									" try {",
									"        const user = await api.addUser(rnd.getUser());",
									"        const user2 = await api.addUser(rnd.getUser());",
									"        const category = await api.addCategory(rnd.getCategory());",
									"        const location1 = rnd.getEventLocation('locA');",
									"        const location2 = rnd.getEventLocation('locB'); // создаём событие в локации А",
									"        let event1 = rnd.getEvent(category.id);",
									"        let event2 = rnd.getEvent(category.id);",
									"        event1.location = {",
									"            lat: location1.latitude,",
									"            lon: location1.longitude",
									"        };",
									"         event2.location = {",
									"            lat: location2.latitude,",
									"            lon: location2.longitude",
									"        };",
									"        event1.requestModeration = true;",
									"        event1.participantLimit = 2;",
									"        event2.requestModeration = true;",
									"        event2.participantLimit = 14;",
									"        event1 = await api.addEvent(user.id, event1);",
									"        event2 = await api.addEvent(user2.id, event2);",
									"        event1 = await api.publishEvent(event1.id);",
									"        event2 = await api.publishEvent(event2.id);",
									"        pm.collectionVariables.set(\"eid\", event1.id)",
									"    } catch(err) {",
									"        console.error(\"Ошибка при подготовке тестовых данных.\", err);",
									"    }",
									"};",
									"",
									"const interval = setInterval(() => {}, 1000);",
									"",
									"setTimeout(async () => ",
									"    {",
									"        try {",
									"            // выполняем наш скрипт",
									"            await main();",
									"        } catch (e) {",
									"            console.error(e);",
									"        } finally {",
									"            clearInterval(interval);",
									"        }",
									"    },  ",
									"    100 ",
									");"
								],
